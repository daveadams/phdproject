<div class="results">
  <table border="0">
    <tr>
      <td style="text-align: right;">
        <b><%=h @earnings_label %></b>
      </td>
      <td>
        <%= number_to_currency(@earnings) %>
      </td>
    </tr>
    <tr>
      <td style="text-align: right;">
        <b><%=h @reported_label %></b>
      </td>
      <td style="border-bottom: 1px solid black;">
        <%= number_to_currency(@reported) %>
      </td>
    </tr>
    <tr>
      <td style="text-align: right;">
        <b>Difference</b>
      </td>
      <td>
        <b><%= number_to_currency(@difference) %></b>
      </td>
    </tr>
<% if @results_ok -%>
  </table>

  <p>You do not owe any additional amounts.</p>
<% else -%>
    <tr>
      <td style="text-align: right;">
        <b><%=h @tax_rate_label %></b>
      </td>
      <td style="border-bottom: 1px solid black;">
        x <%= number_to_percentage(@participant.experimental_group.tax_rate, :precision => 0) %>
      </td>
    </tr>
    <tr>
      <td style="text-align: right;">
        <b><%=h @additional_label %></b>
      </td>
      <td>
        <%= number_to_currency(@additional) %>
      </td>
    </tr>
    <tr>
      <td style="text-align: right;">
        <b>Penalties</b>
      </td>
      <td style="border-bottom: 1px solid black;">
        <%= number_to_currency(@penalties) %>
      </td>
    </tr>
    <tr>
      <td style="text-align: right;">
        <b>Total Amount Due</b>
      </td>
      <td>
        <b><%= number_to_currency(@total) %></b>
      </td>
    </tr>
  </table>

  <p><%=h @message %></p>

<% end %>
</div>

<p>Please proceed to the next round.</p>
<p><%= link_button("Proceed", :action => "end_round") %></p>
