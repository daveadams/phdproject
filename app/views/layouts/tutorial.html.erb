<html>
  <head>
    <meta http-equiv="Expires" content="0" />
    <title>Tutorial</title>
    <%= stylesheet_link_tag("default", :media => :all) %>
    <%= stylesheet_link_tag("tutorial", :media => :all) %>
  </head>

  <body>
    <div class="content">
      <%= yield :layout %>

      <div class="tutorial-navigation">
        <% if prev_page -%>
          <%= link_button("<< Back", :action => prev_page) %>
        <% end -%>
        <% if next_page -%>
          <%= link_button("Next >>", :action => next_page) %>
        <% end -%>
      </div>
    </div>
  </body>
</html>
