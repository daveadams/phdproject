<table class="participants" cellspacing="0">
<tr class="participants-header">
  <th>Participant</th>
  <th>Experimental Group</th>
  <th>Current Phase</th>
  <th>Current Page</th>
  <th>Cash</th>
  <th>Station</th>
  <th>Notes</th>
</tr>

<%= render :partial => "current_participant",
          :collection => this_session.current_participants %>

<% if this_session.unseen_participants.length > 0 %>
  <tr class="unseen-participants-header">
    <th colspan="7">Unseen Participants</th>
  </tr>
<% end %>

<%= render :partial => "unseen_participant",
          :collection => this_session.unseen_participants %>

<% if this_session.complete_participants.length > 0 %>
  <tr class="complete-participants-header">
    <th colspan="7">Complete Participants</th>
  </tr>
<% end %>

<%= render :partial => "complete_participant",
           :collection => this_session.complete_participants %>
</table>
<% if this_session.current_participants.length > 0 and
       not this_session.is_locked_down %>
  <% javascript_tag do %>
    $('lockdown-button').disabled = false
  <% end %>
<% end %>
